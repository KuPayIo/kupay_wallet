.new-page{
    display: flex;
    flex-direction: column;
}
.content{
    background: #fff;
    margin: 20px;
    font-size: 28px;
    color: #666666;
    border-radius: 12px;
    box-shadow: 0px 0px 2px 0px rgba(0, 0, 0, 0.2);
    transform: translateZ(-1px);
}
.title{
    text-align: center;
    font-weight: 600;
    color: #222222;
    padding: 100px 0;
    font-size: 48px;
}