<div class="new-page" ev-back-click="backPrePage" w-class="new-page">
    <app-components1-topBar-topBar>{"title":{{it1.cfgData.topBarTitle}} }</app-components1-topBar-topBar>
    <div style="overflow-x: hidden;overflow-y: auto;flex: 1 0 0;-webkit-overflow-scrolling: touch;">
        <div w-class="content">
            <div w-class="title">{{it1.cfgData.title1}}</div>
            <div style="padding: 0 30px;line-height: 50px;">
                <p style="margin-bottom: 50px;">{{it1.cfgData.content1[0]}}</p>
                <p>{{it1.cfgData.content1[1]}}</p>
            </div>
            <div w-class="title">{{it1.cfgData.title2}}</div>
            <div style="padding: 0 30px 50px;line-height: 50px;">
                <p>{{it1.cfgData.content2[0]}}</p>
                <p>{{it1.cfgData.content2[1]}}</p>
                <p>{{it1.cfgData.content2[2]}}</p>
                <p>{{it1.cfgData.content2[3]}}</p>
                <p>{{it1.cfgData.content2[4]}}</p>
            </div>
        </div>
    </div>
</div>