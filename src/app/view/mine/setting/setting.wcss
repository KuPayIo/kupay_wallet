.content{
    margin: 30px 20px;
}
.userHead{
    height: 150px;
    border-radius: 12px;
    background-color: rgba(255, 255, 255, 1);
    box-shadow: 0px 0px 2px 0px rgba(0, 0, 0, 0.2);
    display: flex;
    align-items: center;
    margin-bottom: 30px;
}
.headImg{
    width: 120px;
    height: 120px;
    margin-left: 30px;
    margin-right: 20px;
}
.headName{
    font-size: 32px;
    color: #222222;
    flex: 1;
}