.new-page{
    display: flex;
    flex-direction: column;
    padding: 30px 20px 20px;
}
.bottom-box{
    margin-top: 30px;
    background-color: #fff;
    border-radius: 12px;
    box-shadow: 0px 0px 2px 0px rgba(0, 0, 0, 0.2);
    position: relative;
    flex: 1 0 0;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    padding-bottom: 35px;
}
.choose-image-container{
    height: 533px;
    border-radius: 12px;
    border: 1px solid #888;
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    overflow: hidden;
}
.choose-image-text{
    color: #222;
    font-size: 38px;
}
.ui-html{
    width: 100%;
    height: 100%;
    position: relative;
}
.image-psw-container{
    position: absolute;
    bottom: 10px;
    left: 10px;
    right: 10px;
    height: 112px;
    opacity: 0.8;
    border-radius: 12px;
    background-color: #fff;
}
.input-father{
    height: 110px;
}
.btn{
    margin: 0 40px;
}