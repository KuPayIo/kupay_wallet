.new-page{
    background: #f9f9f9;
    display: flex;
    flex-direction: column;
}
.content{
    background: #fff;
    margin: 30px 20px;
    border-radius: 12px;
}
.itemDetail{
    padding: 30px;
    line-height: 40px;
    color: rgba(136, 136, 136, 1);
    font-size: 28px;
    border-bottom: 1px solid rgb(219, 219, 229);
}

.itemCompleteDetail{
    color:rgb(50, 148, 230);
    display:flex;
    justify-content:space-between;
}