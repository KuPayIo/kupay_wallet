.item{
    height: 120px;
    line-height: 120px;
    padding: 0 30px;
    border-bottom: 1px solid #DBDBE5;
}
.itemName{
    height: 45px;
    line-height: 45px;
    color: rgba(34, 34, 34, 1);
    font-size: 32px;
    padding-top: 15px;
    margin-bottom: 10px;
    font-weight: 600;
}
.itemTime{
    height: 33px;
    line-height: 33px;
    color: rgba(136, 136, 136, 1);
    font-size: 24px;
}
.itemRight{
    float: right;
}